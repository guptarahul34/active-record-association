<h1> User Profile </h1>

        
   


<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">Description</th>
      <th scope="col">Date</th>
    </tr>
  </thead>
  <tbody>
  <% @user_profile.each do |event| %>
    <tr>
      <th scope="row"><%= event.id %></th>
      <td><%= event.name %></td>
      <td><%= event.description %></td>
      <td><%= event.event_date %></td>
    </tr>
  <% end %>
  </tbody>
</table>

